{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@shared/*": ["src/components/shared/*"],
      "@screens/*": ["src/screens/*"],
      "@components/*": ["src/components/*"],
      "@models/*": ["src/models/*"],
      "@constants/*": ["src/constants/*"],
      "@data/*": ["src/data/*"]
    }
  }
}
